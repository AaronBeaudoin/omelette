{
  "name": "vite-plugin-ssr-vue-starter",
  "version": "0.1.0",
  "scripts": {
    "dev:vite": "vite",
    "dev:wrangler": "wrangler dev wrangler/worker/handle-functions",
    "dev:manifest": "chokidar . -i **/_manifest.ts -c \"npm run build:manifest\"",
    "dev": "npm run build:manifest && concurrently npm:dev:*",
    "build:vite": "vite build",
    "build:manifest": "ts-node wrangler/build-manifest",
    "build:bundle": "npm run build:manifest && ts-node wrangler/build-bundle",
    "build": "npm run build:vite && npm run build:bundle",
    "preview:wrangler": "wrangler dev wrangler/worker.mjs --no-bundle",
    "preview": "npm run build && npm run preview:wrangler",
    "deploy:wrangler": "wrangler publish wrangler/worker.mjs --no-bundle",
    "deploy": "npm run build && npm run deploy:wrangler"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "vite-plugin-ssr": "^0.4.31",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.16.0",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@iconify-json/heroicons-outline": "^1.1.2",
    "@iconify-json/heroicons-solid": "^1.1.2",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/node": "^18.6.1",
    "@types/object-hash": "^2.2.1",
    "@vercel/ncc": "^0.34.0",
    "@vercel/node": "^2.5.6",
    "@vitejs/plugin-vue": "^3.0.1",
    "chalk": "^4.1.2",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.4.0",
    "dedent-js": "^1.0.1",
    "esbuild": "^0.15.0",
    "express": "^4.18.1",
    "glob": "^8.0.3",
    "object-hash": "^3.0.0",
    "sirv": "^2.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "unocss": "^0.45.7",
    "unplugin-auto-import": "^0.10.2",
    "unplugin-icons": "^0.14.7",
    "unplugin-vue-components": "^0.21.2",
    "vite": "^3.0.3",
    "vite-plugin-md": "^0.20.2",
    "vite-svg-loader": "^3.4.0",
    "wrangler": "^2.0.29"
  }
}
