{
  "name": "vite-plugin-dist-importer",
  "version": "0.3.2",
  "description": "Automatically & statically import `dist/server/` assets",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsc --watch",
    "build": "rm -rf dist/ && tsc",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/",
    "*.d.ts",
    "*.js"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^15.12.2",
    "rollup": "^2.75.5",
    "typescript": "^4.3.5",
    "vite": "^2.4.4"
  }
}
